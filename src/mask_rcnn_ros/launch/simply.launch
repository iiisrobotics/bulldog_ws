<launch>
    <node name="mask_rcnn" pkg="mask_rcnn_ros" type="mask_rcnn.py" output="screen">
        <remap from="input" to="camera/color/image_raw" />
        <param name="visualization" value="true" />
    </node>

    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find mask_rcnn_ros)/rviz/mask_rcnn.rviz" />
</launch>
